<VirtualHost *:80>
        ServerName achtelbass.dgx
	ServerAlias achtelbass.dtd.*
    DocumentRoot /home/achtelbass/html
    LogLevel warn
    CustomLog /home/_logs/achtelbass.dgx_access_log combined
    ErrorLog /home/_logs/achtelbass.dgx_error_log

    ErrorDocument 404 /index.html

	WSGIScriptAlias / /home/achtelbass/achtelbass/wsgi.py

	<Directory /home/achtelbass/achtelbass>
		<Files wsgi.py>
			Order deny,allow
			Allow from all
		</Files>
	</Directory>
</VirtualHost>
